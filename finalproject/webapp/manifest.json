{
  "_version": "1.73.1",
  "sap.app": {
    "id": "com.logaligroup.finalproject",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.0",
      "toolsId": "2c114196-fd61-4409-8f94-faab4ec5391f"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.136.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.logaligroup.finalproject.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "com.logaligroup.finalproject.view",
        "async": true
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": ":?query:",
          "target": [
            "TargetMain"
          ]
        },
        {
          "name": "RouteCreateEmployee",
          "pattern": "employee/create:?query:",
          "target": [
            "TargetCreateEmployee"
          ]
        },
        {
          "name": "RouteViewEmployees",
          "pattern": "employee/view:?query:",
          "target": [
            "TargetViewEmployees"
          ]
        },
        {
          "name": "RouteSignOrder",
          "pattern": "orders/sign:?query:",
          "target": [
            "TargetOrderSign"
          ]
        }
      ],
      "targets": {
        "TargetMain": {
          "id": "Main",
          "name": "Main"
        },
        "TargetCreateEmployee": {
          "id": "CreateEmployee",
          "name": "CreateEmployee"
        },
        "TargetViewEmployees": {
          "name": "ViewEmployees"
        },
        "TargetOrderSign": {
          "name": "OrderSign"
        }
      }
    },
    "rootView": {
      "viewName": "com.logaligroup.finalproject.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
